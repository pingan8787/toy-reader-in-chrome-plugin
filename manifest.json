{
	"manifest_version": 3,
	"name": "article-reader-plugin",
	"version": "1.0.0",
	"description": "程序员文章阅读辅助插件",
	"icons": {
		"16": "./image/wx-16.png",
		"48": "./image/wx-48.png",
		"128": "./image/wx-128.png"
	},
	"permissions": ["storage", "activeTab", "scripting"],
	"background": {
	  "service_worker": "js/background.js"
	},
	"action": {
	  "default_popup": "page/popup.html",
	  "default_icon": {
		"16": "./image/wx-16.png",
		"48": "./image/wx-48.png",
		"128": "./image/wx-128.png"
	  }
	}
}